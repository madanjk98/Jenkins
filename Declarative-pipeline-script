currentBuild.displayName="Madan-#"+currentBuild.number
pipeline {
    agent any
	environment {
	  DOCKER_CREDENTIALS=credentials('9164998583-docker-hub')
	  }
    stages {
        stage('Git checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/madanjk98/Jenkins.git'
            }
        }
        stage('mvn build') {
            steps {
                sh "mvn clean package"
            }
        }
	
		
		
    }
}
